{
  "kind": "collectionType",
  "collectionName": "tour_trong_nuocs",
  "info": {
    "singularName": "tour-trong-nuoc",
    "pluralName": "tour-trong-nuocs",
    "displayName": "Tour Trong Nước",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "TourName": {
      "type": "string"
    },
    "TourImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "TourPrice": {
      "type": "biginteger"
    },
    "TourAvatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "TourContent": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "TourDescription": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "TourNote": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "TourDetail": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "Departure": {
      "type": "date"
    },
    "Vehicle": {
      "type": "enumeration",
      "enum": [
        "Xe ô tô",
        "Máy Bay",
        "Xe 16 chỗ",
        "Xe 45 chỗ",
        "Du thuyền"
      ],
      "default": "Xe ô tô"
    },
    "Duration": {
      "type": "string"
    },
    "StartingPosition": {
      "type": "string"
    },
    "Airlines": {
      "type": "string"
    }
  }
}
